<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="bala.comQM_Portal.controller.qmdashboard"
	xmlns:html="http://www.w3.org/1999/xhtml">
<App id="app">
		<pages>
			<Page title="Quality portal">
				<headerContent>
					<Button icon="sap-icon://decline" press="onPress"/>
				</headerContent>
				<content>
				<IconTabBar
		id="idIconTabBarNoIcons"
		expanded="{device>/isNoPhone}"
		class="sapUiResponsiveContentPadding">
					
		<items >
			<IconTabFilter text="Incident list" key="info" >
				<mvc:XMLView viewName="bala.comQM_Portal.view.iplist"/>
			</IconTabFilter>
			<IconTabFilter text="Risk assesment" key="attachments" >
				
			</IconTabFilter>
			<IconTabFilter text="Incident create" key="notes">
				
			</IconTabFilter>
			<IconTabFilter text="Material stock" key="people">
				
			</IconTabFilter>
		</items>
	</IconTabBar>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>